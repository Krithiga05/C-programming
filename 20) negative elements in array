#include <stdio.h>
#define max_size 100
int main()
{
int arr[max_size];
int i,n;
printf("enter size of array:");
scanf("%d",&n);
printf("enter elements in array:");
for(i=0;i<n;i++)
{scanf("%d",&arr[i]);
}
printf("\n all negative elements in an array:");
for(i=0;i<n;i++)
{
if( arr[i]<0)
{printf("%d\t",arr[i]);
}
}
}

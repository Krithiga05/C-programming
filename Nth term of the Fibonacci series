#include<stdio.h>
int fibonacci(int num)
{
    int a=0,b=1,c=0,i;
    if(num<=1)
        return num;
    for(i=2;i<=num;i++)
    {   c=a+b; 
        a=b;     
        b=c;     
    }
    return b;
}
int main()
{
    int num;
    printf("Enter a number: ");
    scanf("%d", &num);
    printf("The %dth Fibonacci number is %d", num, fibonacci(num));
    
} 

# include<stdio.h>
# include<conio.h>
# include<math.h>
main (){
   float a,b,c,r1,r2,d;
   printf ("enter A value : ");
   scanf ("%f",&a);
   printf ("enter B value : ");
   scanf ("%f",&b);
   printf ("enter C value : ");
   scanf ("%f",&c);
   d=((b*b)-(4*a*c));
   if (d>0){
      r1 = -b+sqrt(d)/(2*a);
      r2 = -b-sqrt (d)/(2*a);
      printf ("The real roots are \n r1 = %f and r2 = %f",r1,r2);
   }
   else if (d==0){
      r1 =-b/(2*a);
      r2 =-b/(2*a);
      printf ("roots are equal \n r1 = %f and r2 = %f",r1,r2);
   }
   else
      printf("Roots are imaginary");
   getch();
}

#include<stdio.h>
void swap(int a, int b){
	int c;
	c=a;
	a=b;
	b=c;
	printf("After swapping \n A = %d \n B = %d",a,b);
}
int main()
{
	int a;
	int b;
	printf("Enter A value :");
	scanf("%d",&a);
	printf("Enter B value :");
	scanf("%d",&b);
	printf("Before swapping \n A = %d \n B = %d \n",a,b);
	swap(a,b);	
}

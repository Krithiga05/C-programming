#include<stdio.h>
void swap(int*n1,int*n2);
int main()
{
	int num1,num2;
	printf("Enter first number\n");
	scanf("%d",&num1);
	printf("Enter second number\n");
	scanf("%d",&num2);
	printf("Before swapping \n First number = %d \n Second number = %d\n",num1,num2);
	swap(&num1,&num2);
	printf("After swapping \n First number = %d \n Second number = %d",num1,num2);
}
void swap(int*n1,int*n2)
    {
    	int temp;
    	temp=*n1;
    	*n1=*n2;
    	*n2=temp;
	}
